{
  "name": "lean-stack/messagebus-bundle",
  "description": "Symfony Bundle for the Lean Messagebus",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Micha Alt",
      "email": "mail@lean-stack.de"
    }
  ],
  "autoload": {
    "psr-4": { "Lean\\Bundle\\MessageBusBundle\\": "src/" }
  },
  "autoload-dev": {
    "psr-4": { "Lean\\Bundle\\MessageBusBundle\\": "tests/" }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.1.1",
    "lean-stack/messagebus": "1.0.x-dev",
    "symfony/http-kernel": "3.3.x-dev",
    "symfony/dependency-injection": "3.3.x-dev",
    "symfony/config": "^3.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0",
    "matthiasnoback/symfony-dependency-injection-test": "^2.0"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
